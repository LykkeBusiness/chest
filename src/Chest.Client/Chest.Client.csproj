<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Version>2.37.0</Version>
    <Description>Provides a client on Chest.
The chest is a metadata service which saves and fetches data against three keys. Category, Collection and Key.
It also provides endpoints to fetch list of all categories, collections and keys in the system.</Description>
    <PackageReleaseNotes>Fixed deserialization issues for nullable enum types</PackageReleaseNotes>
    <IncludeSourceRevisionInInformationalVersion>false</IncludeSourceRevisionInInformationalVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Lykke.Contracts" Version="2.0.3" />
    <PackageReference Include="LykkeBiz.HttpClientGenerator" Version="5.6.2" />
    <PackageReference Include="Refit" Version="5.2.4" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="5.0.0" />
  </ItemGroup>

</Project>
