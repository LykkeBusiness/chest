{
  "urls": "http://*:80;",
  "serilog": {
    "minimumLevel": {
      "default": "Information"
    },
    "writeTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "rollingInterval": "Day",
                "path": "logs/Chest/Chest-docker.log",
                "fileSizeLimitBytes": null
              }
            }
          ]
        }
      }
    ]
  }
}